<table class ='table'>
<thead>
    <tr>
        <th>Title</th>
        <th>Content</th>
        <th>Imagen</th>
        <th>Creado</th>
    </tr>
    </thead>

    <tbody>
        
        <% @posts.each do |p| %>
        <tr>
            <td><%= p.title %></td>
            <td><%= p.content %></td>
            <td><%= p.image_url %></td>
            <td><%= p.created_at %></td>
        </tr>
        <% end %>
        
    </tbody>
</table>
